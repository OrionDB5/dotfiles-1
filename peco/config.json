{
    "Prompt": "[peco]",
    "Style": {
        "Basic": ["on_default", "white"],
        "SavedSelection": ["bold", "on_white", "black"],
        "Selected": ["bold", "on_black", "white"],
        "Query": ["green", "bold"],
        "Matched": ["black", "on_yellow"]
